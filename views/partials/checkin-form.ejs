<form>

    <p>
        <label>Seleccione una tienda</label>
        <select class="form-control" name="id_tienda">
            <%for ( var i=0; i < tiendas.length; i++){%>
            <option value="<%= tiendas[i].id %>"><%= tiendas[i].nombre %></option>
            <%}%>
        </select>
    </p>

    <label>Nombre Empleado</label>
    <p>
        <input type="text" class="form-control" name="nombres" placeholder="Nombre...">
    </p>
    <p>
        <label>Apellido Empleado</label>
        <input type="text"  class="form-control" name="apellido" placeholder="Apellido...">
    </p>

    <p>
        <button type="submit" class="btn btn-primary"><i class="fa fa-search"></i> Buscar</button>
    </p>

</form>

<div id = "search_results">

</div>



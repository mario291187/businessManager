<form id="dev_item">
<hr>
<% if (items.length> 0){
for (var i=0; i< items.length; i++){%>

<div class="panel panel-default">
    <div class="panel-body">
        <form>
            <h3>Artículo: <span style="color: #e0e0e0;"><%= items[i].articulo %></span> </h3>
            <p style="color:#9e9e9e"><strong>Modelo:</strong> <%= items[i].modelo %> | <strong>Proveedor: </strong><%= items[i].nombre_prov %> | <strong>Registros: </strong><%= items[i].num_arts %> | <strong>Fecha: </strong><span style="color:#E91E63"><%= items[i].fecha.toDateString() %></span></p>
            <hr>
            <!--<input class="form-control" type="text" value="<%= items[i].articulo %>" name="articulo" disabled>
            <input type="text" name="user_id" value="<%=user.id%>" data-user_id="<%=user.id%>" hidden>
            <input type="text" name="item_id" data-item_id="<%= items[i].id %>" value="<%= items[i].id %>" hidden>
             -->
            <h3><span style="color:#9e9e9e"><strong>Precio Unitario:  </strong></span><span style="color:#E91E63"><strong><%= items[i].precio %></strong></span></h3>
            <br>
        </form>
    </div>
</div>
<%}%>
<button type="submit" data-action="back_items" data-toggle="modal" data-target="#genericModal" class="list-group-item"><i class="fa fa-exchange"></i> <strong>Devolver</strong></button>
<%}else{%>
<div class="alert alert-warning">
    La búsqueda no retornó resultados
</div>
<%}%>
</form>
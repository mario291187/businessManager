<form id="find">

    <div class="row">
        <div class="col-sm-6">
            <label>NÃºmero de nota</label>
            <p>
                <input type="number" class="form-control" name="id_note" value="1">
            </p>
        </div>
        <div class="col-sm-6">
            <label>Tienda</label>
            <select class="form-control" name = "id_tienda">
                <%for (var i=0; i< tiendas.length; i ++){%>
                <option value="<%= tiendas[i].id %>"><%=tiendas[i].nombre%></option>
                <%}%>

            </select>
        </div>

    </div>

    <div class="row">
        <div class="col-sm-6">
            <label>Fecha inicial</label>
            <div class="form-group">
                <div class='input-group'>
                    <input type="text" name="fecha_inicial" id="notes_datepicker1" class="form-control" required>
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>

        </div>
        <div class="col-sm-6">
            <label>Fecha final</label>
            <div class="form-group">
                <div class='input-group'>
                    <input type="text" name="fecha_final" id="notes_datepicker2" class="form-control" required>
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>

        </div>
    </div>
    <p>
        <button type="submit" class="btn btn-primary"> <i class="fa fa-search"></i> Buscar</button>
    </p>
</form>
<br>
<div id = "search_results">
</div>

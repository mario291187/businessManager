<!-- Reports -->

<h2>Reporte de <%= data.type%></h2>


<table class="table table-striped">
    <thead>
    <tr>

        <% for (var i=0; i< data.names.length; i++){%>

        <th><%= data.names[i] %></th>

        <%}%>
    </tr>
    </thead>
    <tbody>


    <% for (var i=0; i< data.rows.length; i ++){%>
    <tr>

        <%
        for ( var prop in data.rows[i]){
        %>
        <td> <%= data.rows[i][prop] %> </td>
        <%} %>

    </tr>
    <%}%>
    </tbody>
</table>